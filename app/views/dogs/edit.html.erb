<h1>Dogs App by Helen</h1>
<h2>Edit an Existing Dog</h2>
<%= form_for @dog do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :gender %>
  <%= f.text_field :gender %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.label :vaccinated? %>
  <%= f.check_box :vaccinated? %>

  <%= f.submit %>
<% end %>

<%= link_to "Back to All Dogs", dogs_path %>
